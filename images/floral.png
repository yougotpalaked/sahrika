<svg xmlns="http://www.w3.org/2000/svg" width="200" height="200" viewBox="0 0 200 200">
  <defs>
    <pattern id="floral" patternUnits="userSpaceOnUse" width="50" height="50">
      <!-- Flower 1 -->
      <circle cx="25" cy="25" r="8" fill="white" fill-opacity="0.2"/>
      <circle cx="25" cy="25" r="4" fill="white" fill-opacity="0.3"/>
      <!-- Petals -->
      <ellipse cx="25" cy="15" rx="5" ry="8" fill="white" fill-opacity="0.3"/>
      <ellipse cx="35" cy="25" rx="8" ry="5" fill="white" fill-opacity="0.3"/>
      <ellipse cx="25" cy="35" rx="5" ry="8" fill="white" fill-opacity="0.3"/>
      <ellipse cx="15" cy="25" rx="8" ry="5" fill="white" fill-opacity="0.3"/>
      <!-- Small decorative dots -->
      <circle cx="10" cy="10" r="2" fill="white" fill-opacity="0.2"/>
      <circle cx="40" cy="10" r="2" fill="white" fill-opacity="0.2"/>
      <circle cx="10" cy="40" r="2" fill="white" fill-opacity="0.2"/>
      <circle cx="40" cy="40" r="2" fill="white" fill-opacity="0.2"/>
    </pattern>
  </defs>
  <rect width="200" height="200" fill="url(#floral)"/>
</svg>